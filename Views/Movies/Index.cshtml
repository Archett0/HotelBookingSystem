@model IEnumerable<Movie>
@{
	ViewBag.Title = "Movies";
	Layout = "~/Views/Shared/_Layout.cshtml";
}
<br/>
<h2>电影列表</h2>
<table class="table table-bordered table-hover">
	<thead>
	<tr>
		<th>Movie</th>
	</tr>
	</thead>
	<tbody>
	@foreach (var movie in Model)
	{
		<tr>
			<td>@movie.Name</td>
		</tr>
	}
	</tbody>
</table>
